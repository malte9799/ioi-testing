{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string"
		},
		"data": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"version": {
						"type": "string",
						"pattern": "^\\d+\\.\\d+\\.\\d+$"
					},
					"changes": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"type": {
									"type": "string",
									"enum": ["feat", "fix", "misc", "remove", "change"]
								},
								"desc": {
									"type": "string"
								}
							},
							"required": ["type", "desc"],
							"additionalProperties": false
						}
					}
				},
				"required": ["version", "changes"],
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}
